{
  "author" : "scrappy zhang",
  "language" : "zh-hans",
  "plugins": [
    "disqus",
    "-search",
    "search-pro",
    "font-settings",
    "-sharing",
    "sharing-plus",
    "livereload",
    "github",
    "editlink",
    "prism",
    "highlight",
    "baidu",
    "splitter",
    "sitemap",
    "katex",
    "include-codeblock",
    "splitter",
    "mermaid",
    "tbfed-pagefooter",
    "emphasize",
    "toggle-chapters",
    "expandable-chapters-small",
    "anchors",
    "anchor-navigation-ex",
    "copy-code-button",
    "include-csv"
  ],
  "pluginsConfig": {
    "disqus": {
      "shortName": "用python带你进入数据世界"
    },
    "github": {
      "url": "https://github.com/ScrappyZhang/python_web_Crawler_DA_ML_DL"
    },
    "tbfed-pagefooter": {
      "copyright":"Copyright &copy scrappy_zhang 2018",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
  "search-pro": {
    "cutWordLib": "nodejieba",
    "defineWord" : ["Gitbook Use"]
    },
  "anchor-navigation-ex": {
      "isRewritePageTitle": true,
      "isShowTocTitleIcon": true,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
  }
  }
}